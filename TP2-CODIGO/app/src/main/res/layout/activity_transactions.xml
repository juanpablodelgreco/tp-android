<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayoutTransactionsId"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView3id"
        android:layout_width="419dp"
        android:layout_height="32dp"
        android:background="#9FEC47"
        android:text="TRANSACCIONES"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="-8dp"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/getTransactionsButtonId"
        android:layout_width="172dp"
        android:layout_height="59dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="456dp"
        android:text="Ver Transacciones"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buyButtonId"
        android:layout_width="172dp"
        android:layout_height="59dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="536dp"
        android:text="Comprar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/transactionsText"
        android:layout_width="363dp"
        android:layout_height="345dp"
        android:layout_marginBottom="56dp"
        android:background="#CCCACA"
        android:maxLines="15"
        android:textAlignment="center"
        android:textColor="#87CF34"
        app:layout_constraintBottom_toTopOf="@+id/getTransactionsButtonId"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>